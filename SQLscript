Sales Tax with AcctClass REM.sql
WITH CBITEMScte as
(SELECT DDAENB "Company Number",	DHGGNB Invoice#,date(TIMESTAMP( (DHIVNB + 19000000) CONCAT'000000'))  "Ship Date",	DDCVNB "ORDER Number", DHDXCD "State",DHB6CD "Zip", DHAQTX "Customer Name",DDARQT "Ship Quantity",
CASE WHEN DDAITX ='' then DDG6CD
ELSE DDAITX
END AS"Item Number",
CASE WHEN DDALTX ='' then DDAAF2
ELSE DDALTX
end AS "Item Description",
CASE 
WHEN DDAAF2 LIKE '%REPAIR%' THEN 'Repair'
WHEN DDAAF2 LIKE '%REBATE%' THEN 'FG'
WHEN DDAAF2 LIKE '%EXTENDED%' THEN 'EXT'
WHEN DDAAF2 LIKE '%EWNDR%' THEN 'EXT'
WHEN DDAAF2 LIKE '%ENCRYPTION%' THEN 'SW'
WHEN DDAAF2 LIKE '%OPTION%' THEN 'SW'
WHEN DDAAF4 = 1 THEN 'RPR/WAR'
ELSE TRIM(ITACIM)
END AS chargeType,
CASE 
when DHDXCD = 'NY'
THEN 'New York'
when DHDXCD = 'FL'
THEN 'Florida'
when DHDXCD = 'CA' AND LEFT(DHB6CD,5) in 
(91901,	91902,	91903,	91905,	91906,	91908,	91909,	91910,	91911,	91912,	91913,	91914,	91915,	91916,	91917,	91921,	91931,	91932,	91933,	91934,	91935,	91941,	91942,	91943,	91944,	91945,	91946,	91947,	91948,	91950,	91951,	91962,	91963,	91976,	91977,	91978,	91979,	91980,	91987,	91990,	92003,	92004,	92007,	92008,	92009,	92010,	92011,	92013,	92014,	92018,	92019,	92020,	92021,	92022,	92023,	92024,	92025,	92026,	92027,	92028,	92029,	92030,	92033,	92036,	92037,	92038,	92039,	92040,	92046,	92049,	92051,	92052,	92054,	92055,	92056,	92057,	92059,	92060,	92061,	92064,	92065,	92066,	92067,	92068,	92069,	92070,	92071,	92072,	92074,	92075,	92078,	92079,	92081,	92082,	92083,	92084,	92085,	92086,	92088,	92090,	92091,	92092,	92093,	92096,	92101,	92102,	92103,	92104,	92105,	92106,	92107,	92108,	92109,	92110,	92111,	92112,	92113,	92114,	92115,	92116,	92117,	92118,	92119,	92120,	92121,	92122,	92123,	92124,	92126,	92127,	92128,	92129,	92130,	92131,	92132,	92133,	92134,	92135,	92136,	92137,	92138,	92139,	92140,	92142,	92143,	92145,	92147,	92149,	92150,	92152,	92153,	92154,	92155,	92158,	92159,	92160,	92161,	92162,	92163,	92164,	92165,	92166,	92167,	92168,	92169,	92170,	92171,	92172,	92173,	92174,	92175,	92176,	92177,	92178,	92179,	92182,	92184,	92186,	92187,	92190,	92191,	92192,	92193,	92194,	92195,	92196,	92197,	92198,	92199)
THEN 'San Diego County'
when DHDXCD = 'CA' AND LEFT(DHB6CD,5) not in 
(91901,	91902,	91903,	91905,	91906,	91908,	91909,	91910,	91911,	91912,	91913,	91914,	91915,	91916,	91917,	91921,	91931,	91932,	91933,	91934,	91935,	91941,	91942,	91943,	91944,	91945,	91946,	91947,	91948,	91950,	91951,	91962,	91963,	91976,	91977,	91978,	91979,	91980,	91987,	91990,	92003,	92004,	92007,	92008,	92009,	92010,	92011,	92013,	92014,	92018,	92019,	92020,	92021,	92022,	92023,	92024,	92025,	92026,	92027,	92028,	92029,	92030,	92033,	92036,	92037,	92038,	92039,	92040,	92046,	92049,	92051,	92052,	92054,	92055,	92056,	92057,	92059,	92060,	92061,	92064,	92065,	92066,	92067,	92068,	92069,	92070,	92071,	92072,	92074,	92075,	92078,	92079,	92081,	92082,	92083,	92084,	92085,	92086,	92088,	92090,	92091,	92092,	92093,	92096,	92101,	92102,	92103,	92104,	92105,	92106,	92107,	92108,	92109,	92110,	92111,	92112,	92113,	92114,	92115,	92116,	92117,	92118,	92119,	92120,	92121,	92122,	92123,	92124,	92126,	92127,	92128,	92129,	92130,	92131,	92132,	92133,	92134,	92135,	92136,	92137,	92138,	92139,	92140,	92142,	92143,	92145,	92147,	92149,	92150,	92152,	92153,	92154,	92155,	92158,	92159,	92160,	92161,	92162,	92163,	92164,	92165,	92166,	92167,	92168,	92169,	92170,	92171,	92172,	92173,	92174,	92175,	92176,	92177,	92178,	92179,	92182,	92184,	92186,	92187,	92190,	92191,	92192,	92193,	92194,	92195,	92196,	92197,	92198,	92199)
THEN 'California Outside San Diego County'
ELSE 'Outside California'
END AS sanDiego, 
'', DDDPVA "Total", 
CASE 
WHEN TAXSU = 'C0001'
THEN 'Taxable'
WHEN TAXSU = 'NOTAX'
THEN 'Reseller' 
WHEN TAXSU = 'C0003'
THEN 'Reseller'
WHEN TAXSU = ''
THEN 'Taxable' 
ELSE TAXSU
END AS taxCategory
FROM AMFLIBB.MBDDRESH SH
JOIN AMFLIBB.MBDHRESM SM ON (SH.DDCVNB=SM.DHCVNB AND SH.DDK4NB=SM.DHK4NB) 
JOIN AMFLIBB.CUSMAS CM ON  CM.CUSNO=SH.DDCANB
LEFT JOIN AMFLIBB.ITEMASA SO ON SH.DDAITX=SO.ITNBR 
WHERE DDALDT >= 1141231 AND ddk4nb=DHK4NB 
UNION all
SELECT F9.F9AENB "Company Number",	F9.F9GGNB Invoice#,date(TIMESTAMP( (DHIVNB + 19000000) CONCAT'000000'))  "Invoice Date",	F9.F9CVNB "ORDER Number",DHDXCD "State",DHB6CD "Zip", DH.DHAQTX "Customer Name",F9.F9AAD2 "Ship Quantity",F9.F9BXTX	"Item Number",F9.F9AA73 "Item Description", 'Freight' AS chargeType,
CASE 
when DHDXCD = 'NY'
THEN 'New York'
when DHDXCD = 'FL'
THEN 'Florida'
when DHDXCD = 'CA' AND LEFT(DHB6CD,5) in 
(91901,	91902,	91903,	91905,	91906,	91908,	91909,	91910,	91911,	91912,	91913,	91914,	91915,	91916,	91917,	91921,	91931,	91932,	91933,	91934,	91935,	91941,	91942,	91943,	91944,	91945,	91946,	91947,	91948,	91950,	91951,	91962,	91963,	91976,	91977,	91978,	91979,	91980,	91987,	91990,	92003,	92004,	92007,	92008,	92009,	92010,	92011,	92013,	92014,	92018,	92019,	92020,	92021,	92022,	92023,	92024,	92025,	92026,	92027,	92028,	92029,	92030,	92033,	92036,	92037,	92038,	92039,	92040,	92046,	92049,	92051,	92052,	92054,	92055,	92056,	92057,	92059,	92060,	92061,	92064,	92065,	92066,	92067,	92068,	92069,	92070,	92071,	92072,	92074,	92075,	92078,	92079,	92081,	92082,	92083,	92084,	92085,	92086,	92088,	92090,	92091,	92092,	92093,	92096,	92101,	92102,	92103,	92104,	92105,	92106,	92107,	92108,	92109,	92110,	92111,	92112,	92113,	92114,	92115,	92116,	92117,	92118,	92119,	92120,	92121,	92122,	92123,	92124,	92126,	92127,	92128,	92129,	92130,	92131,	92132,	92133,	92134,	92135,	92136,	92137,	92138,	92139,	92140,	92142,	92143,	92145,	92147,	92149,	92150,	92152,	92153,	92154,	92155,	92158,	92159,	92160,	92161,	92162,	92163,	92164,	92165,	92166,	92167,	92168,	92169,	92170,	92171,	92172,	92173,	92174,	92175,	92176,	92177,	92178,	92179,	92182,	92184,	92186,	92187,	92190,	92191,	92192,	92193,	92194,	92195,	92196,	92197,	92198,	92199)
THEN 'San Diego County'
when DHDXCD = 'CA' AND LEFT(DHB6CD,5) not in 
(91901,	91902,	91903,	91905,	91906,	91908,	91909,	91910,	91911,	91912,	91913,	91914,	91915,	91916,	91917,	91921,	91931,	91932,	91933,	91934,	91935,	91941,	91942,	91943,	91944,	91945,	91946,	91947,	91948,	91950,	91951,	91962,	91963,	91976,	91977,	91978,	91979,	91980,	91987,	91990,	92003,	92004,	92007,	92008,	92009,	92010,	92011,	92013,	92014,	92018,	92019,	92020,	92021,	92022,	92023,	92024,	92025,	92026,	92027,	92028,	92029,	92030,	92033,	92036,	92037,	92038,	92039,	92040,	92046,	92049,	92051,	92052,	92054,	92055,	92056,	92057,	92059,	92060,	92061,	92064,	92065,	92066,	92067,	92068,	92069,	92070,	92071,	92072,	92074,	92075,	92078,	92079,	92081,	92082,	92083,	92084,	92085,	92086,	92088,	92090,	92091,	92092,	92093,	92096,	92101,	92102,	92103,	92104,	92105,	92106,	92107,	92108,	92109,	92110,	92111,	92112,	92113,	92114,	92115,	92116,	92117,	92118,	92119,	92120,	92121,	92122,	92123,	92124,	92126,	92127,	92128,	92129,	92130,	92131,	92132,	92133,	92134,	92135,	92136,	92137,	92138,	92139,	92140,	92142,	92143,	92145,	92147,	92149,	92150,	92152,	92153,	92154,	92155,	92158,	92159,	92160,	92161,	92162,	92163,	92164,	92165,	92166,	92167,	92168,	92169,	92170,	92171,	92172,	92173,	92174,	92175,	92176,	92177,	92178,	92179,	92182,	92184,	92186,	92187,	92190,	92191,	92192,	92193,	92194,	92195,	92196,	92197,	92198,	92199)
THEN 'California Outside San Diego County'
ELSE 'Outside California'
END AS sanDiego, 
F9.F9AA73 "Item Description",F9.F9DDVA "Total", 
CASE 
WHEN TAXSU = 'C0001'
THEN 'Taxable'
WHEN TAXSU = 'NOTAX'
THEN 'Reseller' 
WHEN TAXSU = 'C0003'
THEN 'Reseller'
WHEN TAXSU = ''
THEN 'Taxable' 
ELSE TAXSU
END AS taxCategory
FROM AMFLIBB.MBF9CPP F9
JOIN AMFLIBB.MBDHRESM DH ON F9.F9GGNB=DH.DHGGNB
JOIN AMFLIBB.CUSMAS CM ON  CM.CUSNO=DH.DHCANB
WHERE F9ALDT >= 1141231
UNION all 
SELECT GFAENB "Company Number",	GFGGNB Invoice#,date(TIMESTAMP( (DHIVNB + 19000000) CONCAT'000000'))  "Invoice Date",	GFCVNB "ORDER Number",DHDXCD "State",DHB6CD "Zip", DHAQTX "Customer Name",'1' "Ship Quantity",'Tax'	"Item Number",GFACHH "Item Description", 'Tax' AS chargeType,
CASE 
when DHDXCD = 'NY'
THEN 'New York'
when DHDXCD = 'FL'
THEN 'Florida'
when DHDXCD = 'CA' AND LEFT(DHB6CD,5) in 
(91901,	91902,	91903,	91905,	91906,	91908,	91909,	91910,	91911,	91912,	91913,	91914,	91915,	91916,	91917,	91921,	91931,	91932,	91933,	91934,	91935,	91941,	91942,	91943,	91944,	91945,	91946,	91947,	91948,	91950,	91951,	91962,	91963,	91976,	91977,	91978,	91979,	91980,	91987,	91990,	92003,	92004,	92007,	92008,	92009,	92010,	92011,	92013,	92014,	92018,	92019,	92020,	92021,	92022,	92023,	92024,	92025,	92026,	92027,	92028,	92029,	92030,	92033,	92036,	92037,	92038,	92039,	92040,	92046,	92049,	92051,	92052,	92054,	92055,	92056,	92057,	92059,	92060,	92061,	92064,	92065,	92066,	92067,	92068,	92069,	92070,	92071,	92072,	92074,	92075,	92078,	92079,	92081,	92082,	92083,	92084,	92085,	92086,	92088,	92090,	92091,	92092,	92093,	92096,	92101,	92102,	92103,	92104,	92105,	92106,	92107,	92108,	92109,	92110,	92111,	92112,	92113,	92114,	92115,	92116,	92117,	92118,	92119,	92120,	92121,	92122,	92123,	92124,	92126,	92127,	92128,	92129,	92130,	92131,	92132,	92133,	92134,	92135,	92136,	92137,	92138,	92139,	92140,	92142,	92143,	92145,	92147,	92149,	92150,	92152,	92153,	92154,	92155,	92158,	92159,	92160,	92161,	92162,	92163,	92164,	92165,	92166,	92167,	92168,	92169,	92170,	92171,	92172,	92173,	92174,	92175,	92176,	92177,	92178,	92179,	92182,	92184,	92186,	92187,	92190,	92191,	92192,	92193,	92194,	92195,	92196,	92197,	92198,	92199)
THEN 'San Diego County'
when DHDXCD = 'CA' AND LEFT(DHB6CD,5) not in 
(91901,	91902,	91903,	91905,	91906,	91908,	91909,	91910,	91911,	91912,	91913,	91914,	91915,	91916,	91917,	91921,	91931,	91932,	91933,	91934,	91935,	91941,	91942,	91943,	91944,	91945,	91946,	91947,	91948,	91950,	91951,	91962,	91963,	91976,	91977,	91978,	91979,	91980,	91987,	91990,	92003,	92004,	92007,	92008,	92009,	92010,	92011,	92013,	92014,	92018,	92019,	92020,	92021,	92022,	92023,	92024,	92025,	92026,	92027,	92028,	92029,	92030,	92033,	92036,	92037,	92038,	92039,	92040,	92046,	92049,	92051,	92052,	92054,	92055,	92056,	92057,	92059,	92060,	92061,	92064,	92065,	92066,	92067,	92068,	92069,	92070,	92071,	92072,	92074,	92075,	92078,	92079,	92081,	92082,	92083,	92084,	92085,	92086,	92088,	92090,	92091,	92092,	92093,	92096,	92101,	92102,	92103,	92104,	92105,	92106,	92107,	92108,	92109,	92110,	92111,	92112,	92113,	92114,	92115,	92116,	92117,	92118,	92119,	92120,	92121,	92122,	92123,	92124,	92126,	92127,	92128,	92129,	92130,	92131,	92132,	92133,	92134,	92135,	92136,	92137,	92138,	92139,	92140,	92142,	92143,	92145,	92147,	92149,	92150,	92152,	92153,	92154,	92155,	92158,	92159,	92160,	92161,	92162,	92163,	92164,	92165,	92166,	92167,	92168,	92169,	92170,	92171,	92172,	92173,	92174,	92175,	92176,	92177,	92178,	92179,	92182,	92184,	92186,	92187,	92190,	92191,	92192,	92193,	92194,	92195,	92196,	92197,	92198,	92199)
THEN 'California Outside San Diego County'
ELSE 'Outside California'
END AS sanDiego, 
GFACHJ "Non Inv Desc",GFGYVA "Total", 
CASE 
WHEN TAXSU = 'C0001'
THEN 'Taxable'
WHEN TAXSU = 'NOTAX'
THEN 'Reseller' 
WHEN TAXSU = 'C0003'
THEN 'Reseller'
WHEN TAXSU = ''
THEN 'Taxable' 
ELSE TAXSU
END AS taxCategory
from AMFLIBB.MBGFCPP
LEFT JOIN AMFLIBB.MBDHRESM ON AMFLIBB.MBGFCPP.GFGGNB=AMFLIBB.MBDHRESM.DHGGNB
JOIN AMFLIBB.CUSMAS CM ON  CM.CUSNO=AMFLIBB.MBDHRESM.DHCANB
WHERE GFGYVA <> 0 and DHIVNB >= 1141231),
TOTALcte as
(SELECT CBITEMScte.*, date(TIMESTAMP( (feghnb + 19000000) CONCAT'000000')) AS "Invoice Date"
FROM CBITEMScte
JOIN amflibb.MBF9RES4 ON Invoice#=feggnb
WHERE "Ship Date" BETWEEN '2022-07-01' AND '2022-09-30')
SELECT sanDiego, chargeType,
sum(CASE WHEN taxCategory = 'Taxable' THEN "Total"
ELSE
NULL
END) AS "Taxable Amt",
sum(CASE WHEN taxCategory = 'Reseller' THEN "Total"
ELSE
NULL
END) AS "Reseller Amt",
sum("Total") AS Total
FROM TOTALcte
GROUP BY sanDiego, chargeType
ORDER BY sanDiego

